    "test:deprecated:integration": "mocha test/js-dash-sdk-deprecated/test.js --exit",
    "test:deprecated:queries": "mocha test/js-dash-sdk-deprecated/queryTest.js --exit",
    "test": "mocha --slow 500 test/test.mjs test/queryTest.mjs --exit"
